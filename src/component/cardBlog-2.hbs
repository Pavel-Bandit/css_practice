{{#with data}}
  <div class='card'>
    <div
      class='card_image'
      style='background-image: url({{photo}})'
    ></div>
    <h2 class='card_title'>{{heading}}</h2>
    <p class='card_text'>{{info}}</p>
    <a
      href='{{linkUrl}}'
      class='card_action'
    >{{linkText}}</a>
  </div>
{{/with}}